{
  "name": "vitality",
  "version": "0.0.1",
  "description": "React Application template",
  "author": "Aleksandar ZajiÄ‡ <alisteroz@gmail.com>",
  "license": "MIT",
  "keywords": [
    "app",
    "context",
    "data-loaders",
    "file-based-routing",
    "frontend",
    "hooks",
    "nested-layouts",
    "react",
    "react-location",
    "react-query",
    "react-router",
    "routes",
    "structure",
    "tanstack",
    "template",
    "vite",
    "web",
    "zustand"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm-run-all type-check index",
    "serve": "serve dist",
    "client": "vite build --outDir dist",
    "type-check": "tsc",
    "format": "prettier --write 'src/**/*.{css,html,json,md,mdx,js,jsx,ts,tsx}'",
    "lint": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "prepare": "husky install",
    "test": "vitest --passWithNoTests",
    "test:coverage": "vitest --coverage --passWithNoTests",
    "test:watch": "vitest --watch --passWithNoTests",
    "test:ui": "vitest --ui --watch --passWithNoTests",
    "test:report": "open coverage/lcov-report/index.html",
    "test:lastReport": "npm run vitest:coverage && open coverage/lcov-report/index.html"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.2.6",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@tanstack/react-location": "^3.7.4",
    "@tanstack/react-query": "^4.0.10",
    "@tanstack/react-query-devtools": "^4.0.10",
    "axios": "^0.27.2",
    "framer-motion": "^7.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.34.0",
    "react-icons": "^4.4.0",
    "react-intl": "^6.0.5",
    "yup": "^0.32.11",
    "zustand": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@hookform/resolvers": "^2.9.7",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^14.4.2",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vitejs/plugin-react": "^1.3.2",
    "@vitest/ui": "^0.21.1",
    "c8": "^7.12.0",
    "esbuild": "^0.14.48",
    "esbuild-register": "^3.3.3",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lint-staged": "^13.0.3",
    "msw": "^0.44.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "serve": "^13.0.4",
    "typescript": "^4.7.4",
    "vite": "^3.0.5",
    "vitest": "^0.21.1"
  }
}
